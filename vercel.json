{
	"version": 2,
	"buildCommand": "npm run build",
	"installCommand": "npm install",
	"outputDirectory": "dist",
	"builds": [
		{
			"src": "dist/app.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["dist/**"]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/app.js"
		}
	]
}
