{
	"name": "zentro-backend",
	"version": "1.0.0",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"dev": "tsx --watch ./src/server.ts",
		"start": "node dist/server.js",
		"build": "babel src --out-dir dist --extensions '.ts'",
		"type-check": "tsc --noEmit",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "jest ",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint",
			"prettier --check"
		],
		"*.{json,yml,yaml,md}": [
			"prettier --check"
		]
	},
	"keywords": [],
	"author": "Jaizzer",
	"license": "MIT",
	"description": "",
	"dependencies": {
		"dotenv": "^17.2.2",
		"express": "^5.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.28.3",
		"@babel/core": "^7.28.4",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-typescript": "^7.27.1",
		"@eslint/js": "^9.36.0",
		"@tsconfig/node22": "^22.0.2",
		"@types/express": "^5.0.3",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.5.2",
		"@types/supertest": "^6.0.3",
		"eslint": "^9.36.0",
		"eslint-plugin-perfectionist": "^4.15.0",
		"husky": "^9.1.7",
		"jest": "^30.1.3",
		"lint-staged": "^16.2.1",
		"prettier": "^3.6.2",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.4",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0"
	}
}
