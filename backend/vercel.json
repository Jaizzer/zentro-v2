{
	"version": 2,
	"builds": [
		{
			"src": "./dist/app.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "./dist/app.js"
		}
	],
	"outputDirectory": "dist",
	"installCommand": "npm install",
	"buildCommand": "npm run build",
	"devCommand": "node --watch dist/app.js"
}
