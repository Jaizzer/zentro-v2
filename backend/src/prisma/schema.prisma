generator client {
    provider = "prisma-client-js"
    output   = "../generated/prisma"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       String @id @unique @default(cuid())
    username String @unique

    @@map("users")
}

model File {
    id   String @id @default(cuid())
    name String

    @@map("files")
}
